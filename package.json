{
  "name": "kerox",
  "version": "1.0.0",
  "description": "A network stressing tool that uses HTTP(S) requests with optional proxies",
  "author": "RubyDevil",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "local": "npm run build && npm install -g ../kerox"
  },
  "bin": {
    "kerox": "./bin/kerox-cli.js"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.0",
    "axios": "^1.6.1",
    "overpaint.js": "^2.0.1",
    "shelljs": "^0.8.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "typescript": "^5.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RubyDevil/Kerox.git"
  },
  "keywords": [
    "dos",
    "ddos",
    "network",
    "stress",
    "network-stress",
    "network-stressing",
    "network-stresser",
    "http",
    "https",
    "proxy",
    "proxies"
  ],
  "bugs": {
    "url": "https://github.com/RubyDevil/Kerox/issues"
  },
  "homepage": "https://github.com/RubyDevil/Kerox#readme"
}
