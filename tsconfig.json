{
   "include": [
      "**/*.ts"
   ],
   "compilerOptions": {
      "sourceMap": true,
      "resolveJsonModule": true,
      "target": "esnext",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
   }
}